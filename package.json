{
  "name": "volunteer-backend",
  "private": true,
  "version": "1.0.0",
  "description": "Volunteer Backend",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start --watch --preserveWatchOutput",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "cli:dev": "ts-node -r tsconfig-paths/register src/cli.ts",
    "cli:prod": "node dist/cli.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:int": "pnpm run test:container:restart && dotenv -e .env.test -- pnpx prisma migrate reset --preview-feature --force --skip-generate --skip-seed",
    "pretest:e2e": "pnpm run test:container:restart && dotenv -e .env.test -- pnpx prisma migrate reset --preview-feature --force --skip-generate --skip-seed",
    "test:container:restart": "docker compose --env-file .env.test -f docker-compose.test.yml down && docker compose --env-file .env.test -f docker-compose.test.yml up -d && sleep 3",
    "test:int": "dotenv -e .env.test -- jest --runInBand --detectOpenHandles --no-cache --bail --config ./jest-int.json",
    "test:e2e": "dotenv -e .env.test -- jest --runInBand --detectOpenHandles --no-cache --bail --config ./jest-e2e.json",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "migrate:deploy": "pnpx prisma migrate deploy --preview-feature",
    "migrate:status": "pnpx prisma migrate status --preview-feature",
    "migrate:resolve": "pnpx prisma migrate resolve --preview-feature",
    "migrate:squash": "ts-node scripts/squash-migrations",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:reset:force": "prisma migrate reset --preview-feature --force",
    "prisma:studio": "pnpx prisma studio",
    "prisma:generate": "pnpx prisma generate",
    "prisma:generate:watch": "pnpx prisma generate --watch",
    "prisma:seed": "dotenv -e .env -- ts-node prisma/seed/seed.ts",
    "prisma:reset-and-seed": "dotenv -e .env -- pnpx prisma migrate reset --force && ts-node prisma/seed/seed.ts",
    "prisma:test": "ts-node prisma/scripts/test.ts",
    "doc:serve": "pnpx compodoc -p tsconfig.json -w -s -r 8001",
    "doc:generate": "pnpx compodoc -p tsconfig.json",
    "prepare": "is-ci || husky install",
    "pre-commit": "lint-staged",
    "testa": "ts-node scripts/test.ts"
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.417.0",
    "@aws-sdk/lib-storage": "^3.417.0",
    "@casl/ability": "^6.5.0",
    "@casl/prisma": "^1.4.1",
    "@googlemaps/google-maps-services-js": "^3.3.38",
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.2.6",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.6",
    "@nestjs/event-emitter": "^2.0.2",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.6",
    "@nestjs/platform-socket.io": "^10.2.6",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/swagger": "^7.1.12",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/websockets": "^10.2.6",
    "@prisma/client": "^5.3.1",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.2.3",
    "cache-manager-ioredis-yet": "^1.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "csv": "^6.3.3",
    "csv-parse": "^5.5.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "ejs": "^3.1.9",
    "firebase-admin": "^11.10.1",
    "form-data": "^4.0.0",
    "geolib": "^3.3.4",
    "ioredis": "^5.3.2",
    "joi": "^17.10.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mailgun.js": "^9.3.0",
    "mime-db": "^1.52.0",
    "mime-types": "^2.1.35",
    "n-digit-token": "^2.2.1",
    "nanoid": "^3.3.6",
    "nest-casl": "^1.8.11",
    "nodemailer": "^6.9.5",
    "otplib": "^12.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "prisma": "^5.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2",
    "swagger-ui-express": "5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@compodoc/compodoc": "^1.1.21",
    "@faker-js/faker": "^8.1.0",
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.6",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.87",
    "@types/bcrypt": "^5.0.0",
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/k6": "^0.46.3",
    "@types/lodash": "^4.14.198",
    "@types/mime-db": "^1.43.1",
    "@types/mime-types": "^2.1.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.17.18",
    "@types/nodemailer": "^6.4.10",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  }
}
